/* Contact Section */
#contact-container {
  display: grid;
  grid-template-rows: 30vh 50vh auto;
}

.contact-title {
  text-align: center;
  padding: 50px 0 0 0;
  font-size: 1.5em;
  color: #172b4d;
}

.contact-form {
  display: flex;
  flex-direction: column;
  padding: 30px 20px 30px 20px;
  gap: 10px;
}

.form-btn {
  align-self: center;
}

.contact-icones {
  justify-content: center;
}

.input-form {
  outline: none;
  border: none;
  border-bottom: 1px solid gray;
}

.form-text-area {
  background: #fbf8f7;
  border: none;
  padding: 20px;
}
